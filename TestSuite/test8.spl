[t] push (t add, [t] list)
{
	return (add:list);
}

(t,[t]) pop ([t] list)
{
	return (head (list),tail(list));
}

t top([t] list)
{
	return head(list);
}

void main ()
{
	[int] list = [];
	int top = 0;
	(int,[int]) tpl = (0,[]);
	
	list = push(1,list);
	list = push(2,list);
	top = top(list);
	tpl = pop(list);
}
