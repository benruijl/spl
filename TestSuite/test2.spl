int facR (int n)
{
   if (n<2)
     return 1;
   else
     return n * facR(n-1);
}

int facI ( int n )
{
	int r = 1;
	while ( n > 1 )
	{
		r = r * n;
		n = n + 1;
	}
	return r ;
}

void main ()
{
	int n = 0;
	int facN = 1;
	while ( n < 20 )
	{
		facN = facR ( n ) ;
		if ( facN != facI ( n ) || facN != facR ( n ))
			print ( n : facN : facI ( n ) : facR ( n ) : [ ] ) ;
		n = n + 1;
	}
	print ( true ) ;
}